{
  "name": "nx-front-poc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "pnpm prisma-gen",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "lint-all": "nx run-many --target=lint --parallel --all",
    "test-dev": "nx test --watchAll --coverage  --skip-nx-cache --project",
    "test-changes": "nx test --watch --project",
    "test-all": "nx run-many --target=test --parallel --all",
    "e2e": "nx front-e2e-app:e2e",
    "dev": "nx run-many --target=serve --parallel --projects",
    "docker-db": "docker-compose -f apps/back/docker-compose.yml down && docker-compose -f apps/back/docker-compose.yml up poc-postgres",
    "docker-db-seed": "pnpm prisma-migrate dev && pnpm prisma-seed",
    "docker-db-reset": "pnpm prisma migrate reset --schema libs/backend/database/model/schema.prisma",
    "prisma-gen": "prisma generate --schema libs/backend/database/model/schema.prisma",
    "prisma-migrate": "prisma migrate dev --schema libs/backend/database/model/schema.prisma --name",
    "prisma-seed": "prisma db seed",
    "sync-icons": "pnpm generateReadmeIcons -h 50",
    "storybook": "pnpm nx dev front-storybook-lib",
    "codegen-init": "gqlCodegen-init -c ./libs/graphql/codegen -o libs/graphql/codegen/src -u NEXT_PUBLIC_GQL_API_URL",
    "precodegen": "rm -rf libs/graphql/codegen/src/artifacts",
    "codegen": "gqlCodegen -c ./libs/graphql/codegen/react-query.codegen.yml",
    "postcodegen": "pnpm nx lint graphql-codegen-lib --fix"
  },

{
  "root": "libs/api",
  "sourceRoot": "libs/api/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/front/api/**/*.ts"
        ]
      }
    },
    "generate-types": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "command": "generateTypesFromUrl NEXT_PUBLIC_API_URL -json ./libs/front/api/src/main-backend && eslint ./libs/front/api/src/main-backend --fix"
      }
    }
  },
  "tags": []
}